<?xml version="1.0" encoding="UTF-8"?>
<manifest>

<!-- build -->
  <project path="build/make" name="build" remote="lite" > 
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/kati" name="build_kati" remote="lite" />
  <project path="build/soong" name="build_soong" remote="lite" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

<!-- bionic -->
  <project path="bionic" name="bionic" remote="lite" />

<!-- bootable -->
  <project path="bootable/recovery" name="bootable_recovery" remote="lite" />

<!-- device-->
  <project path="device/lite/sepolicy" name="device_lite_sepolicy" remote="lite" />

<!-- Manifest -->
  <project name="manifest" path="manifest" remote="lite" />

<!-- vendor -->
  <project name="vendor_lite" path="vendor/lite" remote="lite" />

<!-- LiteOS -->
  <project name="LiteOS_apps_prebuilt" path="LiteOS/apps/prebuilt" remote="lite" />
  <project name="LiteOS_overlay" path="LiteOS/overlay" remote="lite" />

</manifest>